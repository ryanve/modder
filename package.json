{
  "name": "modder",
  "description": "mod your modules",
  "version": "0.1.0",
  "homepage": "https://github.com/ryanve/modder",
  "license": "MIT",
  "author": "Ryan Van Etten",
  "keywords": ["modding", "overloading", "hooks", "extension", "ender"],
  "main": "modder.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ryanve/modder.git"
  },
  "dependencies": {},
  "optionalDependencies": {},
  "devDependencies": {
    "grunt-contrib-uglify": "^0.8.0",
    "grunt-contrib-concat": "^0.5.0",
    "grunt-contrib-jshint": "^0.11.0",
    "universal": "^0.1.0"
  },
  "jshintConfig": {
    "asi": true,
    "boss": true,
    "browser": true,
    "debug": true,
    "devel": true,
    "eqnull": true,
    "evil": true,
    "expr": true,
    "globals": {
      "define": true,
      "ender": true 
    },
    "lastsemic": true,
    "latedef": "nofunc",
    "laxcomma": true,
    "maxcomplexity": 15,
    "maxerr": 10,
    "maxlen": 115,
    "maxparams": 4,
    "node": true,
    "sub": true,
    "supernew": true,
    "trailing": true,
    "undef": true,
    "unused": true,
    "worker": true
  }
}